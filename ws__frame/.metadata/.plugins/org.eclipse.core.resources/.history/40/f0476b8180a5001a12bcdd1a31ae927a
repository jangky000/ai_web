package dev.mvc.shopping_cart;

import java.util.List;

public interface Shopping_cartDAOInter {
  
  /**
   * <xmp>
   * 장바구니 등록
   * <insert id="create" parameterType="Shopping_cartVO">
   * <xmp>
   * @return
   */
  public int create(Shopping_cartVO shopping_cartVO);

  /**
   * <xmp>
   * 장바구니 목록
   * <select id="list" resultType="PorderVO">
   * <xmp>
   * @return
   */
  public List<PorderVO> list();


  /**
   * <xmp>
   * 장바구니 수정
   * <update id="update" parameterType="PorderVO">
   * <xmp>
   * @return
   */
  public int update(PorderVO porderVO);

  /**
   * <xmp>
   * 장바구니 삭제
   * <delete id="delete" parameterType="int">
   * <xmp>
   * @return
   */
  public int delete(int porderno);

}
