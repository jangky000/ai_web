  <insert id="create" parameterType="PorderVO">
    INSERT INTO porder(contentsno, memberno, cateno, title, content, web, ip,
                                     passwd, word, rdate)
    VALUES(contents_seq.nextval, #{memberno}, #{cateno}, #{title}, #{content}, #{web}, #{ip},
                #{passwd}, #{word}, sysdate)
  </insert>
</mapper> 